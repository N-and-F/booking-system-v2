@using MudBlazor

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.body1">@Notes</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Cancel">Close</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    [Parameter] public string Notes { get; set; }

    void Cancel() => MudDialog.Cancel();
} 